{
  "name": "@woongjang/won-ui",
  "version": "0.0.24",
  "description": "ui library",
  "author": "WON",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woongjang/won-ui.git"
  },
  "keywords": [
    "won-ui",
    "won",
    "react",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/woongjang/won-ui/issues"
  },
  "homepage": "https://github.com/woongjang/won-ui#readme",
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "eslint": "^8.15.0",
    "eslint-config-acme": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "peerDependencies": {},
  "dependencies": {}
}
