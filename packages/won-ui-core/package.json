{
  "name": "@won-ui/core",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woongjang/won-ui.git"
  },
  "keywords": [
    "won-ui",
    "won",
    "react",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/woongjang/won-ui/issues"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@won-ui/tsconfig": "*",
    "eslint": "^8.15.0",
    "eslint-config-acme": "*",
    "react": "^18.1.0",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
